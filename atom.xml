---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Code Story</title>
	<link href="http://www.code-play.net/atom.xml" rel="self" />
	<link href="http://www.code-play.net/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.code-play.net/</id>
	<author>
		<name>David Gageot</name>
		<email>david@gageot.net</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://www.code-play.net{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://www.code-play.net{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>